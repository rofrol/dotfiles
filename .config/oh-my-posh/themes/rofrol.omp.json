{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#6c98f6",
    "blush": "#DA627D",
    "green": "#338a80",
    "plum": "#9A348E",
    "purple": "#b000dc",
    "salmon": "#ff966d",
    "sky": "#86BBD8",
    "tan": "#CC3802",
    "teal": "#047E84",
    "teal_blue": "#33658A",
    "white": "#FFFFFF",
    "yellow": "#f1d354"
  },
  "transient_prompt": {
    "template": " üïá \uf43a {{ .Segments.Time.CurrentDate | date .Segments.Time.Format }} ‚ùØ {{ .Segments.Path.Path }} ‚ùØ \n$ ",
    "foreground": "#000000"
  },
  "blocks": [
    {
      "type": "rprompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{ if .Env.PNPPSHOST }} \uf8c5 {{ .Env.PNPPSHOST }} {{ end }}",
          "foreground": "p:white",
          "background": "p:tan",
          "type": "text",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Env.PNPPSSITE }} \uf2dd {{ .Env.PNPPSSITE }}{{ end }}",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:teal",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": "{{ if .Env.PNPPSSITE }}¬†{{ end }}",
          "foreground": "p:white",
          "background": "p:teal",
          "type": "text",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b0",
          "template": " üïá ",
          "foreground": "p:white",
          "background": "p:purple",
          "type": "text",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04"
          },
          "template": " \uf43a {{ .CurrentDate | date .Format }} ",
          "foreground": "p:white",
          "background": "p:yellow",
          "type": "time",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\ue5fe ",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "style": "agnoster"
          },
          "template": " {{ .Path }} ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:blue",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:salmon",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:salmon{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#f17c37{{ end }}",
            "{{ if gt .Behind 0 }}#89d1dc{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:green",
          "type": "node",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue235 {{ .Full }} ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:sky",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue753 {{ .Full }} ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:blush",
          "type": "angular",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ue3bf ",
          "foreground": "#cd5e42",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "$ ",
          "foreground": "#000000",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
